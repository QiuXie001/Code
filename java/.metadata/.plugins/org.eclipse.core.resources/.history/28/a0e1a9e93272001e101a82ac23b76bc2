interface Animal {
		    void sleep();
		}
class Cat implements Animal {
    @Override
    public void sleep() {
        System.out.println("Cat is sleeping");
    }
    
    public void catchMouse() {
        System.out.println("Cat is catching a mouse");
    }
}
public class Example04_4 {
	public static void main(String[] args) {
		
		

	}

}
