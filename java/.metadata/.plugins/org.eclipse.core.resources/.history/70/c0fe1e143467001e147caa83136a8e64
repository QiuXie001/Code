package com.v1;

public class T5 {
	String name;
	int age;
	public T5(String name,int age) {
		this.name=name;
		this.age=age;
	}
	public void speak() {
		System.out.println(name);
		System.out.println(age);
	}
}
class MainClass {
public static void main(String[] arg) {
	T5 stu1=new T5("正义",36);
	stu1.speak();
	
}
}
